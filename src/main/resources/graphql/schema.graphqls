type Book {
    id: String!,
    isbn: String!,
    publishDate: String!,
    authorId: Int!,
    title: String!,
    publisherId: String!,
    price: Float!

}

type Query {
    books: [Book]!,
    findBookByAuthorId(authorId:Int): Book
}

type Mutation {
    addBook(id: String, isbn: String, publishDate: String, authorId: Int, title: String, publisherId: String, price: Float): Book
    updateBook(id: String, isbn: String, publishDate: String, authorId: Int, title: String, publisherId: String, price: Float): Book
    deleteBookById(id: String!): Boolean

}